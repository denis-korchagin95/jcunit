  directive requirement-list direct-directive

requirement-list:
  requirement
  requirement-list requirement

requirement:
  direct-directive requirement-content-opt

requirement-content: text

directive:
    direct_directive directive-param-opt

direct-directive: '@' name

directive-param: '(' string ')'

 '@' name '(' string ')'

name: [a-zA-Z_][a-zA-Z0-9_]*

text: .*

string: '"' [any character except '"' and '\n']* '"'
